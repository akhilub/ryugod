# for loop
for i = 0 till 100 do
	if i % 3 == 0 and i % 5 == 0 then
		print("fizzbuzz")
	else if i % 3 == 0 then
		print("fizz")
	else if i % 5 == 0 then
		print("buzz")
	else
		print(i)
	end
end

# while loop
i = 0
while i != 5 do
	print("Hello! " + tostr(i)) # 'tostr' converts i  to string 'toint' does opposite
	i = i + 1
end
