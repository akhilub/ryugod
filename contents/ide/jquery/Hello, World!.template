<!-- JQuery(제이쿼리) Hello, World! 예제 -->
<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <title>JQuery</title>
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
        <script type="text/javascript">
            $(document).ready(function(){
                $("#app").html("<h1>Hello, World!</h1>")
            })
        </script>
    </head>
    <body>
        <div id="app">
        </div>    
    </body>
</html>