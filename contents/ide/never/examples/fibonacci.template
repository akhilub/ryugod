func fibL(n : long) -> long
{
    (n == 0L) ? 0L : (n == 1L) ? 1L : fibL(n - 1L) + fibL(n - 2L)
}

func fib(n : int) -> int
{
    (n == 0) ? 0 : (n == 1) ? 1 : fib(n - 1) + fib(n - 2)
}

func main() -> int 
{
    print(fib(14));

